            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/colorBackground" />

        <TextView style="@style/tv_KeDoc" />
    </LinearLayout>

    <!-- Start Header -->
    <TextView style="@style/tv_KeNgang" />
